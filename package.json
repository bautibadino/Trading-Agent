{
  "name": "trading-bot-market-core",
  "version": "1.0.0",
  "description": "Clientes HTTP/WebSocket m\u00ednimos para Binance",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/server/index.js",
    "dev": "tsc -p tsconfig.json --watch & node --watch dist/src/server/index.js",
    "prews": "npm run build",
    "ws": "npx --yes node@22 --dns-result-order=ipv4first dist/scripts/ws-listener.js",
    "prerest": "npm run build",
    "rest": "node dist/scripts/rest-example.js",
    "prelive": "npm run build",
    "live": "node dist/scripts/live-scalping-backtest.js",
    "prelive:eth": "npm run build",
    "live:eth": "node dist/scripts/live-scalping-backtest.js --preset=eth-scalp",
    "prelive:btc": "npm run build",
    "live:btc": "node dist/scripts/live-scalping-backtest.js --preset=btc-scalp-fast",
    "prelive:stream": "npm run build",
    "live:stream": "npx --yes node@22 --dns-result-order=ipv4first dist/scripts/live-scalping-backtest.js --mode=stream",
    "live:help": "node dist/scripts/live-scalping-backtest.js --help",
    "prews:futures": "npm run build",
    "ws:futures": "npx --yes node@22 --dns-result-order=ipv4first dist/scripts/ws-futures-ai.js",
    "ws:futures:eth": "npx --yes node@22 --dns-result-order=ipv4first dist/scripts/ws-futures-ai.js --symbol=ETHUSDT",
    "ws:futures:1m": "npx --yes node@22 --dns-result-order=ipv4first dist/scripts/ws-futures-ai.js --interval=1m",
    "ws:futures:5m": "npx --yes node@22 --dns-result-order=ipv4first dist/scripts/ws-futures-ai.js --interval=5m",
    "ws:futures:15m": "npx --yes node@22 --dns-result-order=ipv4first dist/scripts/ws-futures-ai.js --interval=15m",
    "ws:futures:30m": "npx --yes node@22 --dns-result-order=ipv4first dist/scripts/ws-futures-ai.js --interval=30m",
    "ws:futures:1h": "npx --yes node@22 --dns-result-order=ipv4first dist/scripts/ws-futures-ai.js --interval=1h",
    "ws:futures:4h": "npx --yes node@22 --dns-result-order=ipv4first dist/scripts/ws-futures-ai.js --interval=4h",
    "prelogs": "npm run build",
    "logs": "node dist/scripts/read-logs.js",
    "start:all": "./scripts/start-all-timeframes.sh",
    "stop:all": "./scripts/stop-all-timeframes.sh"
  },
  "keywords": [
    "trading",
    "binance",
    "market-data",
    "websocket"
  ],
  "author": "Bautista Badino",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "trading-signals": "^7.0.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.9.0",
    "@types/ws": "^8.5.12",
    "typescript": "^5.4.5"
  }
}
